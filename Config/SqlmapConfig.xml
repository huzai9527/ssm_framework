<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <typeAliases>
        <!--单个别名的定义-->
        <typeAlias type="pojo.Moive" alias="moive"/>
        <!--批量别名的定义-->
        <package name="pojo" />
    </typeAliases>

    <!-- 注册xml文件 -->
    <mappers>
        <mapper resource="mapper/moivesMapper.xml"/>
    </mappers>
</configuration>

